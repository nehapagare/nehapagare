    <script>
    document.addEventListener("DOMContentLoaded", function() {
        const counters = document.querySelectorAll('.kpi-counter');
        const options = {
            threshold: 0.6
        };

        const runCounter = (entry) => {
            const el = entry.target;
            const endVal = parseInt(el.getAttribute('data-value'), 10);

            if (!el.classList.contains('counted') && !isNaN(endVal)) {
                const counter = new window.countUp.CountUp(el, endVal, {
                    duration: 2,
                    separator: ',',
                });

                if (!counter.error) {
                    counter.start();
                    el.classList.add('counted');
                } else {
                    console.error(counter.error);
                }
            }
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    runCounter(entry);
                }
            });
        }, options);

        counters.forEach(counter => {
            observer.observe(counter);
        });
    });
    </script>
