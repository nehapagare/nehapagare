        function addDotLabels() {
            document.querySelectorAll('.owl-dot').forEach((dot, index) => {
                dot.setAttribute('aria-label', `Go to slide ${index + 1}`);
                dot.setAttribute('title', `Go to slide ${index + 1}`);
            });
